{
  "name": "@akitain/sandjs",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0-rc.1",
  "description": "Sunburst Advanced Node Data",
  "main": "dist/sandjs.mjs",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "prepublishOnly": "npm run build",
    "clean:test": "rimraf dist-tests",
    "test:build": "npm run clean:test && tsc --project tsconfig.test.json",
    "test": "npm run test:build && node --test dist-tests/tests/*.js",
    "verify": "npm run test && npm run build",
    "dev": "node scripts/dev-server.mjs",
    "clean:docs": "rimraf temp docs/api",
    "docs:extract": "api-extractor run --local",
    "docs:build": "npm run build && npm run clean:docs && npm run docs:extract && api-documenter markdown --input-folder temp --output-folder docs/api && node scripts/postprocess-docs.mjs",
    "clean:bench": "rimraf dist-benchmarks",
    "bench:build": "npm run clean:bench && tsc --project tsconfig.bench.json",
    "bench": "npm run bench:build && node dist-benchmarks/benchmarks/index.js",
    "bench:layout": "npm run bench:build && node dist-benchmarks/benchmarks/layout.bench.js",
    "bench:render": "npm run bench:build && node dist-benchmarks/benchmarks/render.bench.js",
    "bench:navigation": "npm run bench:build && node dist-benchmarks/benchmarks/navigation.bench.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aqu1tain/sandjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Aqu1tain/sandjs/issues"
  },
  "homepage": "https://github.com/Aqu1tain/sandjs#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.26.34",
    "@microsoft/api-extractor": "^7.52.13",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.5.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.2",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "module": "dist/sandjs.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sandjs.mjs",
      "default": "./dist/sandjs.mjs"
    },
    "./dist/sandjs.iife.min.js": "./dist/sandjs.iife.min.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "unpkg": "dist/sandjs.iife.min.js",
  "jsdelivr": "dist/sandjs.iife.min.js"
}
