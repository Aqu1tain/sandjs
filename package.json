  "homepage": "https://github.com/Aqu1tain/sandjs#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.2",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "module": "dist/sandjs.mjs",
  "types": "dist/index.d.ts",
